{
  "name": "threat-intel-dashboard",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "build": "vite build && cp .assetsignore dist/.assetsignore && wrangler pages functions build --outdir=./dist/_worker.js/",
    "preview": "wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "deploy:dry-run": "npm run build && wrangler deploy --dry-run",
    "typecheck": "tsc --noEmit",
    "cf-typegen": "wrangler types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:create": "wrangler d1 create threat-intel-db",
    "db:init": "wrangler d1 execute threat-intel-db --file=./schema.sql --remote",
    "db:init:local": "wrangler d1 execute threat-intel-db --file=./schema.sql --local",
    "vectorize:create": "wrangler vectorize create threat-embeddings --dimensions=1024 --metric=cosine",
    "vectorize:delete": "wrangler vectorize delete threat-embeddings",
    "kv:create": "wrangler kv namespace create CACHE && wrangler kv namespace create CACHE --preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "fast-xml-parser": "^5.3.2",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.344.0",
    "motion": "^12.23.25",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "recharts": "^2.12.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.14",
    "@cloudflare/workers-types": "^4.20250107.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "typescript": "^5.3.3",
    "vite": "^7.2.6",
    "vitest": "^3.2.4",
    "wrangler": "^4.53.0"
  }
}
