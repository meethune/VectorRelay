{
  "name": "threat-intel-dashboard",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy",
    "cf-typegen": "wrangler types",
    "db:create": "wrangler d1 create threat-intel-db",
    "db:init": "wrangler d1 execute threat-intel-db --file=./schema.sql",
    "db:migrate": "wrangler d1 execute threat-intel-db --file=./schema.sql --remote",
    "vectorize:create": "wrangler vectorize create threat-embeddings --dimensions=768 --metric=cosine"
  },
  "dependencies": {
    "date-fns": "^3.3.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250107.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^7.2.6",
    "wrangler": "^4.53.0"
  }
}
